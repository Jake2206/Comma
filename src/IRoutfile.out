; ModuleID = 'Comma'
source_filename = "Comma"

@fmt = private unnamed_addr constant [4 x i8] c"%d\0A\00", align 1

declare i32 @printf(i8*, ...)

define i32 @main() {
entry:
  %i = alloca i32, align 4
  br label %while

while:                                            ; preds = %while_body, %entry
  %i1 = load i32, i32* %i, align 4
  %tmp = icmp slt i32 %i1, 10
  br i1 %tmp, label %while_body, label %while_end

while_body:                                       ; preds = %while
  %i2 = load i32, i32* %i, align 4
  %printf = call i32 (i8*, ...) @printf(i8* getelementptr inbounds ([4 x i8], [4 x i8]* @fmt, i32 0, i32 0), i32 %i2)
  %i3 = load i32, i32* %i, align 4
  %tmp4 = add i32 %i3, 1
  store i32 %tmp4, i32* %i, align 4
  br label %while

while_end:                                        ; preds = %while
  ret i32 0
}
