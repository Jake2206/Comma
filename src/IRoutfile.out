; ModuleID = 'Comma'
source_filename = "Comma"

@i = global i32 20
@b = global i32 9
@c = global i32 5
@fmt = private unnamed_addr constant [4 x i8] c"%d\0A\00", align 1
@fmt.1 = private unnamed_addr constant [4 x i8] c"%d\0A\00", align 1

declare i32 @printf(i8*, ...)

define i32 @main() {
entry:
  %c = load i32, i32* @c, align 4
  %printf = call i32 (i8*, ...) @printf(i8* getelementptr inbounds ([4 x i8], [4 x i8]* @fmt, i32 0, i32 0), i32 %c)
  store i32 5, i32* @c, align 4
  %test_result = call i32 @test()
  ret i32 0
}

define i32 @test() {
entry:
  %c = load i32, i32* @c, align 4
  %printf = call i32 (i8*, ...) @printf(i8* getelementptr inbounds ([4 x i8], [4 x i8]* @fmt.1, i32 0, i32 0), i32 %c)
  ret i32 0
}
